{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Test",
            "program": "${workspaceRoot}/node_modules/jasmine/bin/jasmine.js",
            "args": [
                "--config=jasmine.json",
            ],
            "stopOnEntry": false,
            "cwd": "${workspaceRoot}",
            "outputCapture": "std",
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "DEBUG": "1"
            }
        },
    ]
}